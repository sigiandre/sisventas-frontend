\\n            },\\r\\n            guardar () {\\r\\n                if (this.validar()){\\r\\n                    return;\\r\\n                }\\r\\n                let header={\\\"Authorization\\\" : \\\"Bearer \\\" + this.$store.state.token};\\r\\n                let configuracion= {headers : header};\\r\\n                if (this.editedIndex > -1) {\\r\\n                    //Código para editar\\r\\n                    //Código para guardar\\r\\n                    let me=this;\\r\\n                    axios.put('api/Categorias/Actualizar',{\\r\\n                        'idcategoria':me.id,\\r\\n                        'nombre': me.nombre,\\r\\n                        'descripcion': me.descripcion\\r\\n                    },configuracion).then(function(response){\\r\\n                        me.close();\\r\\n                        me.listar();\\r\\n                        me.limpiar();                        \\r\\n                    }).catch(function(error){\\r\\n                        console.log(error);\\r\\n                    });\\r\\n                } else {\\r\\n                    //Código para guardar\\r\\n                    let me=this;\\r\\n                    axios.post('api/Categorias/Crear',{\\r\\n                        'nombre': me.nombre,\\r\\n                        'descripcion': me.descripcion\\r\\n                    },configuracion).then(function(response){\\r\\n                        me.close();\\r\\n                        me.listar();\\r\\n                        me.limpiar();                        \\r\\n                    }).catch(function(error){\\r\\n                        console.log(error);\\r\\n                    });\\r\\n                }\\r\\n            },\\r\\n            validar(){\\r\\n                this.valida=0;\\r\\n                this.validaMensaje=[];\\r\\n\\r\\n                if (this.nombre.length<3 || this.nombre.length>50){\\r\\n                    this.validaMensaje.push(\\\"El nombre debe tener más de 